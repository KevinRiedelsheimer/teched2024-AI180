{
  "type": "module",
  "name": "sap-teched24-ai180",
  "version": "1.0.0",
  "private": "true",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map"
  ],
  "engines": {
    "node": "^20"
  },
  "scripts": {
    "start": "node --loader ts-node/esm --watch src/server.ts",
    "local": "node --env-file=.env --loader ts-node/esm src/server.ts",
    "compile": "tsc",
    "dev": "tsc -w",
    "lint": "eslint . && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -c",
    "lint:fix": "eslint . --fix && prettier . --config ../.prettierrc --ignore-path ../.prettierignore -w --log-level error"
  },
  "dependencies": {
    "@sap-ai-sdk/ai-api": "1.0.0",
    "@sap-ai-sdk/foundation-models": "1.0.0",
    "@sap-ai-sdk/orchestration": "1.0.0",
    "@sap-ai-sdk/langchain": "1.0.0",
    "@langchain/core": "0.3.3",
    "@types/express": "4.17.21",
    "@types/node": "22.7.0",
    "express": "4.21.0",
    "dotenv": "16.4.5",
    "ts-node": "10.9.2",
    "typescript": "5.6.2"
  }
}
